{
  "version": 1,
  "projects": {
    "crm": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/crm",
      "sourceRoot": "apps/crm/src",
      "prefix": "akva",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/crm",
            "index": "apps/crm/src/index.html",
            "main": "apps/crm/src/main.ts",
            "polyfills": "apps/crm/src/polyfills.ts",
            "tsConfig": "apps/crm/tsconfig.app.json",
            "aot": true,
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/styles/lib/"]
            },
            "assets": [
              "apps/crm/src/favicon.ico",
              "apps/crm/src/assets",
              "apps/crm/src/mockServiceWorker.js",
              {
                "input": "libs/crm/assets/src/lib",
                "glob": "**/*",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "apps/crm/src/theme.less",
              "libs/shared/styles/lib/style.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/crm/src/environments/environment.ts",
                  "with": "apps/crm/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "crm:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "crm:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "crm:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/crm/tsconfig.app.json",
              "apps/crm/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/crm/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/crm/jest.config.js",
            "tsConfig": "apps/crm/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/crm/src/test-setup.ts"
          }
        }
      }
    },
    "crm-e2e": {
      "root": "apps/crm-e2e",
      "sourceRoot": "apps/crm-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",

    "crm-mocks": { "tags": [] },
    "crm-environments": { "tags": [] },
    "crm-config": { "tags": [] },
    "crm-auth-util": { "tags": [] },
    "crm-layout": { "tags": [] },
    "shared-ui-kit": { "tags": [] }
